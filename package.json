{
  "name": "four-in-line",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "postinstall": "ng build",
    "start": "node server.js",
    "build": "ng build --aot --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "build::electron": "ng build && electron .",
    "package::win": "ng build --prod && electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=src/assets/native/icon.ico --prune=true --out=release-builds",
    "package::mac": "ng build --prod && electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/assets/native/icon.icns --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@angular/service-worker": "^11.0.7",
    "@firebase/app": "^0.6.13",
    "@tensorflow-models/handpose": "0.0.6",
    "@tensorflow/tfjs": "^2.8.5",
    "clipboard": "^2.0.6",
    "custom-electron-titlebar": "^3.2.6",
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0",
    "express": "^4.17.1",
    "fingerpose": "0.0.2",
    "firebase": "^7.0 || ^8.0",
    "handtrackjs": "0.0.13",
    "ngx-clipboard": "^14.0.1",
    "ngx-socket-io": "^3.2.0",
    "path": "^0.12.7",
    "peerjs": "^1.3.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
